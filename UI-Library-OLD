		self:SelectTab(tab, Frame_3)
	end

	function tab:CalcScroll()
		self.Tab[5].CanvasSize = UDim2.new(0, 0, 0, self.pos)
	end

	function tab:AddParagraph(title, subtitle, secondsubtitle)

		local Paragraph = Instance.new("Frame")
		table.insert(tab.ElemBtn, Paragraph)
		local UICorner = Instance.new("UICorner")
		local name = Instance.new("TextLabel")
		table.insert(tab.Texttitle, name)
		local subtext = Instance.new("TextLabel")
		table.insert(tab.Textsubtitle, subtext)
		local secondsubtext = Instance.new("TextLabel")
		table.insert(tab.Textsubtitle, secondsubtext)
		
		local size = 60
		local namepos = 4
		local titlepos = 24
		
		if secondsubtitle == "" then
			size = 45
			titlepos = 24
		end

		Paragraph.Name = "Paragraph"
		Paragraph.Parent = self.Container
		Paragraph.BackgroundColor3 = Color3.fromRGB(225, 225, 225)
		Paragraph.BackgroundTransparency = tab.Theme.BackGroundElemTran
		Paragraph.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Paragraph.BorderSizePixel = 0
		Paragraph.Position = UDim2.new(0, 7, 0, self.pos)
		Paragraph.Size = UDim2.new(0,360, 0, size)

		UICorner.Parent = Paragraph

		name.Name = "name"
		name.Parent = Paragraph
		name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		name.BackgroundTransparency = 1.000
		name.BorderColor3 = Color3.fromRGB(0, 0, 0)
		name.BorderSizePixel = 0
		name.Position = UDim2.new(0.027777778, 0, 0, namepos)
		name.Size = UDim2.new(0, 328, 0, 21)
		name.Font = Enum.Font.FredokaOne
		name.Text = title
		name.TextColor3 = tab.Theme.TextColor
		name.TextSize = 16.000
		name.TextXAlignment = Enum.TextXAlignment.Left

		subtext.Name = "subtext"
		subtext.Parent = Paragraph
		subtext.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		subtext.BackgroundTransparency = 1.000
		subtext.BorderColor3 = Color3.fromRGB(0, 0, 0)
		subtext.BorderSizePixel = 0
		subtext.Position = UDim2.new(0.027777778, 0, 0, titlepos)
		subtext.Size = UDim2.new(0, 332, 0, 14)
		subtext.Font = Enum.Font.FredokaOne
		subtext.Text = subtitle
		subtext.TextColor3 = tab.Theme.SubTtitleTextColor
		subtext.TextSize = 14.000
		subtext.TextXAlignment = Enum.TextXAlignment.Left

		secondsubtext.Name = "second-subtext"
		secondsubtext.Parent = Paragraph
		secondsubtext.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		secondsubtext.BackgroundTransparency = 1.000
		secondsubtext.BorderColor3 = Color3.fromRGB(0, 0, 0)
		secondsubtext.BorderSizePixel = 0
		secondsubtext.Position = UDim2.new(0.027777778, 0, 0, 40)
		secondsubtext.Size = UDim2.new(0, 332, 0, 13)
		secondsubtext.Font = Enum.Font.FredokaOne
		secondsubtext.Text = secondsubtitle
		secondsubtext.TextColor3 = tab.Theme.SubTtitleTextColor
		secondsubtext.TextSize = 14.000
		secondsubtext.TextXAlignment = Enum.TextXAlignment.Left

		table.insert(self.Elements, Paragraph)
		self.pos += 65
		tab:CalcScroll()

		self.SearchData[Paragraph] = title .. " " .. subtitle .. " " .. secondsubtitle
		table.insert(self.Elements, Paragraph)
	end
	function tab:AddSection(title)
		local TextLabel = Instance.new("TextLabel")
		table.insert(tab.Texttitle, TextLabel)

		TextLabel.Parent = self.Container
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.BorderSizePixel = 0
		TextLabel.Position = UDim2.new(0.0291780813, 0, 0, self.pos)
		TextLabel.Size = UDim2.new(0, 193, 0, 21)
		TextLabel.Font = Enum.Font.FredokaOne
		TextLabel.Text = title
		TextLabel.TextColor3 = self.Theme.TextColor
		TextLabel.TextSize = 18.000
		TextLabel.TextXAlignment = Enum.TextXAlignment.Left

		table.insert(self.Elements, TextLabel)
		self.pos += 26
		tab:CalcScroll()

		self.SearchData[TextLabel] = title
		table.insert(self.Elements, TextLabel)
	end

	function tab:AddButton(config)
		local button = {
			Name = config.Name,
			Description = config.Description,
			Callback = config.Callback
		}

		local Button = Instance.new("TextButton")
