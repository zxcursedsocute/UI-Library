			v3.BackgroundColor3 = v.Theme.BackGroundElemCol
		end
		for i3,v3 in pairs(v.BtnImg) do
			v3.Image = v.Theme.ButtonIcon
		end
		for i3,v3 in pairs(v.DropImg) do
			v3.BackgroundColor3 = v.Theme.DropDownColor
		end
		for i3,v3 in pairs(v.DropDownFrameColor) do
			v3.BackgroundColor3 = v.Theme.DropDownFrameColor
		end
		for i3,v3 in pairs(v.ActiveToggleColor) do
			v3.BackgroundColor3 = v.Theme.TogleActivColor
		end
		for i3,v3 in pairs(v.InputTextColor) do
			v3.TextColor3 = v.Theme.InputTextColor
		end
		for i3,v3 in pairs(v.SliderUnderLineColor) do
			v3.BackgroundColor3 = v.Theme.SliderUnderLineColor
		end
		for i3,v3 in pairs(v.Togglebtn) do
			v3.BackgroundColor3 = v.Theme.Togglebtn
		end
		for i3,v3 in pairs(v.Sliderball) do
			v3.BackgroundColor3 = v.Theme.Sliderball
		end
		for i3,v3 in pairs(v.ColorPickerColor) do
			v3.BackgroundColor3 = v.Theme.ColorPickerColoro
		end
	end  
end

return Library
